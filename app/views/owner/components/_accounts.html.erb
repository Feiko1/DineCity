        <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
      </div>
    <%= submit_tag
    <button type="submit" class="btn btn-success">Update</button>
    <button type="submit" class="btn btn-warning">Edit</button>
    </form>
    <% end %>
