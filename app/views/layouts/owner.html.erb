<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>CMS Dine City</title>
  <%= favicon_link_tag 'owners/owner.ico' %>
  <%= csrf_meta_tags %>
  <%= stylesheet_link_tag 'application', media: 'all' %>
  <%= javascript_include_tag "https://cdn.rawgit.com/mahnunchik/markerclustererplus/master/dist/markerclusterer.min.js" %>
  <%= javascript_include_tag "https://maps.google.com/maps/api/js?libraries=places&sensor=false" %>
  
</head>
  <%= render 'owner/layout/navOwner' %>
  <div class="container">
    <div  class="col-md-12">
           <hr/>
           <div class="col-xs-3 col-md-2"> <!-- required for floating -->
             <!-- Nav tabs -->
             <ul class="nav nav-tabs tabs-left sideways">
               <li>
                 <%= link_to owner_dashboards_path do %>
                   <i class="fa fa-tachometer" aria-hidden="true"></i> Dashboard
                 <% end %>
               </li>
               <li class="<%= owner_tab_css_class("account") %>">
                 <%= link_to edit_owner_account_path do %>
                   <i class="fa fa-user" aria-hidden="true"></i> Account
                 <% end %>
                 <!-- <a href="#accounts-v"><i class="fa fa-user" aria-hidden="true"></i> Accounts</a> -->
               </li>
               <li class="<%= owner_tab_css_class("restaurants") %>">
                 <% if current_user.restaurants.count >= 1 %>
                   <%= link_to edit_owner_restaurant_path(current_user.restaurants.first) do %>
                     <i class="fa fa-cutlery" aria-hidden="true"></i> Restaurant
                   <% end %>
                 <% else %>
                   <%= link_to owner_restaurants_path do %>
                     <i class="fa fa-cutlery" aria-hidden="true"></i> Restaurants
                   <% end %>
                 <% end %>
                 <!-- <a href="#accounts-v"><i class="fa fa-user" aria-hidden="true"></i> Accounts</a> -->
               </li>
               <li class="<%= owner_tab_css_class("deals") %>">
                 <%= link_to  owner_deals_path do %>
                   <i class="fa fa-briefcase" aria-hidden="true"></i> Offers
                 <% end %>
               </li>
               <li class="<%= owner_tab_css_class("bookings") %>">
                 <%= link_to owner_bookings_path do %>
                   <i class="fa fa-book" aria-hidden="true"></i> Bookings
                 <% end %>
               </li>
               <!-- <li>
                 <a href="#analytics-v"><i class="fa fa-bar-chart" aria-hidden="true"></i> Analytics</a>
               </li> -->
               <li class="<%= owner_tab_css_class("Billings") %>">
                 <%= link_to owner_billings_path do %>
                   <i class="fa fa-money" aria-hidden="true"></i> Billings
                 <% end %>
               </li>
               <li class="<%= owner_tab_css_class("Support") %>">
                 <%= link_to owner_supports_path do %>
                   <i class="fa fa-phone-square" aria-hidden="true"></i> Support
                 <% end %>
               </li>
             </ul>
           </div>

           <div class="col-xs-12 col-md-9">
             <!-- Tab panes -->
             <div class="tab-content">
               <div class="tab-pane active" id="accounts-v">
                 <%= yield %>
               </div>
             </div>
           </div>
           <div class="clearfix"></div>
    </div>
  </div>
    <div class="container">
    <hr>
    </div>
  <%= javascript_include_tag 'application' %>
  <!-- below is the facebook login javascript -->
 <script>
   window.fbAsyncInit = function() {
     FB.init({
       appId      : '1729792927278815',
       xfbml      : true,
       version    : 'v2.6'
     });
   };

   (function(d, s, id){
      var js, fjs = d.getElementsByTagName(s)[0];
      if (d.getElementById(id)) {return;}
      js = d.createElement(s); js.id = id;
      js.src = "//connect.facebook.net/en_US/sdk.js";
      fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));
 </script>
</body>
 <div class="wrapper">
  <%= render 'owner/layout/footerOwner' %>
</div>
</html>
